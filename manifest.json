{
      "manifest_version": 3,
      "name": "Voice Navigator",
      "version": "0.1.0",
      "description": "AI-powered voice navigation for accessibility",
      "permissions": ["activeTab", "scripting", "tts", "storage"],
      "action": {
        "default_popup": "src/popup/index.html"
      },
      "background": {
        "service_worker": "src/background.js",
        "type": "module"
      },
      "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["src/content.js"]
      }]
    }